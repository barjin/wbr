{
  "name": "@wbr/wbr-cloud",
  "version": "0.1.0",
  "description": "Cloud environment for Smart Workflows Interpret, part of the WBR project",
  "main": "cloud.js",
  "scripts": {
    "start": "node ./build/cloud.js",
    "clean-win": "rmdir .\\build /s/q",
    "clean": "rm -rf ./build",
    "compile": "tsc",
    "copystatic-win": "xcopy .\\public\\ .\\build\\public\\ /E/H/Y",
    "copystatic": "mkdir -p ./build/wbr-cloud/ && cp -r ./public/ ./build/wbr-cloud && mkdir -p ./build/wbr-interpret/ && cp -r ../wbr-interpret/src/*.js ./build/wbr-interpret/src",
    "build-win": "npm run compile & npm run copystatic-win",
    "build-cloud": "npm run compile && npm run copystatic && cp ./package.json ./build "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barjin/wbr.git"
  },
  "keywords": [
    "web",
    "automation",
    "workflow",
    "interpret",
    "scraping"
  ],
  "author": "Jindřich Bär",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/barjin/wbr/issues"
  },
  "homepage": "https://github.com/barjin/wbr#readme",
  "dependencies": {
    "@types/express-fileupload": "^1.1.7",
    "@wbr-project/wbr-interpret": "*",
    "apify": "^2.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "playwright": "^1.16.3",
    "socket.io": "^4.3.2"
  }
}
